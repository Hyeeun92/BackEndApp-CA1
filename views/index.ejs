<html lang="en">
<head>
    <link href="../style.css" rel="stylesheet" type="text/css">
</head>
<body>
    <h3 id="indexTitle">List of Gym Members</h3>
    <ul>
        <% member.forEach((member)=> { %>
            <li>
                <button id="list" onclick="location.href='/<%= member._id %>'">
                    <%= member.name %>
                </button>
            </li>
            <% })%>
    </ul>
    <button id="function" onclick="location.href='/create'">Add Member</button>
</body>

</html>